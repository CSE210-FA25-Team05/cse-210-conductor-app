{
  "source": {
    "include": ["./"],
    "exclude": [
      "./docs",
      "./node_modules",
      "./specs", 
      "./backend/prisma/migrations"
    ],
    "includePattern": ".+\\.(js|jsx|mjs|cjs|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}